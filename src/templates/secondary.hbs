{{#extend "base"}}{{#content "body"}}
<div class="t-secondary c-parallax">
  <div class="c-parallax__images" role="presentation">
    <img class="t-secondary__image-1" src="{{ secondary.parallax_image }}" alt="">
  </div>
  <div class="t-secondary__content-window-1 c-parallax__content-window">
    {{> header }}
    {{> lead }}
  </div>
  <main id="maincontent" class="t-secondary__content-1 c-parallax__content">
    <h2>{{ secondary.heading }}</h2>
    <p>{{{ secondary.paragraphs }}}</p>
  </main>
  {{> footer }}
</div>
{{/content}}{{/extend}}
